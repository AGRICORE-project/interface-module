{
  "build": {
    "appId": "com.ayesa.agricore",
    "productName": "Agricore",
    "directories": {
      "output": "release/"
    },
    "win": {
      "target": "portable"
    },
    "extraResources": [
      "src/assets/img/*"
    ]
  },
  "name": "agricore-interface",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --configuration development --base-href ./ && electron .",
    "build": "ng build",
    "watch": "concurrently ng build --watch --configuration development",
    "test": "ng test",
    "dist": "electron-builder"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/material": "^17.0.0",
    "@angular/material-moment-adapter": "^17.0.0",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@ng-matero/extensions": "^14.6.3",
    "@ngbracket/ngx-layout": "^17.0.0",
    "file-saver": "^2.0.5",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "typescript": "5.2.*",
    "xng-breadcrumb": "^8.0.2",
    "zone.js": "^0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "^17.0.2",
    "@types/d3": "^7.4.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "~4.0.0",
    "electron": "^20.1.2",
    "electron-builder": "^23.6.0",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^3.1.0"
  }
}
